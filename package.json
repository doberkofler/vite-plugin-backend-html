{
	"name": "vite-plugin-backend-html",
	"version": "0.0.1",
	"type": "module",
	"license": "MIT",
	"author": "Dieter Oberkofler",
	"files": [
		"dist"
	],
	"keywords": [
		"vite",
		"vite-plugin",
		"frontend",
		"backend",
		"html"
	],
	"scripts": {
		"dev": "tsdown --watch",
		"build": "tsdown",
		"lint": "eslint --no-cache . && tsc && prettier --check .",
		"test": "vitest run --coverage",
		"ci": "npm run lint && npm run build && npm run test",
		"create-changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
		"pack-local": "rm -f *.tgz && npm run build && npm pack",
		"prepare": "husky"
	},
	"main": "./dist/index.mjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": "./dist/index.mjs",
		"./package.json": "./package.json"
	},
	"engines": {
		"node": ">=22"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/doberkofler/vite-plugin-merge-css.git"
	},
	"homepage": "https://github.com/doberkofler/vite-plugin-merge-css#readme",
	"peerDependencies": {
		"vite": "^7.0.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^20.4.2",
		"@commitlint/config-conventional": "^20.4.2",
		"@eslint/js": "^9.31.0",
		"@types/debug": "^4.1.12",
		"@types/node": "^25.3.0",
		"@vitest/coverage-v8": "^4.0.18",
		"@vitest/eslint-plugin": "^1.6.9",
		"conventional-changelog": "^7.1.1",
		"conventional-changelog-angular": "^8.1.0",
		"conventional-changelog-cli": "^5.0.0",
		"eslint": "^9.31.0",
		"eslint-plugin-jsdoc": "^62.6.1",
		"eslint-plugin-regexp": "^3.0.0",
		"eslint-plugin-unicorn": "^63.0.0",
		"globals": "^17.3.0",
		"husky": "^9.1.7",
		"prettier": "^3.8.1",
		"tsdown": "^0.20.3",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.56.0",
		"vitest": "^4.0.18"
	},
	"dependencies": {
		"debug": "^4.4.3"
	}
}
